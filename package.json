{
  "name": "leancloud-server",
  "description": "Leancloud Server",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "server.js",
  "scripts": {
    "start": "node --harmony server.js",
    "test": "node_modules/.bin/eslint . --fix && NODE_ENV=test node_modules/.bin/mocha test --opts test/mocha.opts",
    "plato": "node_modules/.bin/plato -r -t \"Cloud Code\" -x .json -d report server/**/*.js test/**/*.js app.js cloud.js server.js",
    "lint": "node_modules/.bin/eslint . --fix",
    "deploy": "node_modules/.bin/avoscloud deploy",
    "publish": "node_modules/.bin/avoscloud publish",
    "validate": "npm ls"
  },
  "dependencies": {
    "body-parser": "^1.14.1",
    "cloudmodel": "https://github.com/Hxucaa/cloudmodel.git",
    "colors": "^1.1.2",
    "cookie-parser": "^1.4.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "leanengine": "^0.2.0",
    "leanengine-sniper": "^0.3.1",
    "lodash": "^3.10.1",
    "request": "^2.65.0",
    "validator": "^4.2.1"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "chai-as-promised": "^5.0.0",
    "eslint": "^1.9.0",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.1.0",
    "mocha": "^2.2.4",
    "plato": "^1.4.0",
    "precommit-hook": "^3.0.0"
  },
  "pre-commit": [
    "lint"
  ]
}
